        "### 日期需轉換成 timestamp 的數值"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "uR3wd3T0iOrI",
        "colab_type": "code",
        "colab": {}
      },
      "source": [
        "from matplotlib.pylab import date2num\n",
        "import datetime\n",
        "list_timestamp = []\n",
        "for ele in df_ma['date']:\n",
        "    ele = date2num(datetime.datetime.strptime(ele,'%Y-%m-%d'))\n",
        "    list_timestamp.append(ele)\n",
        "df_ma['date_plot'] = list_timestamp"
      ],
      "execution_count": 0,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "P94rXNQwi2_d",
        "colab_type": "code",
        "outputId": "070d2bfe-8c58-4b74-e21a-f262a5bd37e7",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 221
        }
      },
      "source": [
        "df_ma['date_plot']"
      ],
      "execution_count": 0,
      "outputs": [
        {
          "output_type": "execute_result",
          "data": {
            "text/plain": [
              "0     737426.0\n",
